@model ClinicaVet.GestaoVeterinaria.Models.Atendimento

@{
    ViewData["Title"] = "Atendimentos";
    ViewData["View"] = " - Detalhes";
}

<h1>Detalhes</h1>

<div>
    <h4>Atendimento nº: @Model.Id</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Diagnostico)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Diagnostico)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ObservacoesIniciais)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ObservacoesIniciais)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AtendimentoIniciado)
        </dt>
        <dd class="col-sm-10">
            @Model.AtendimentoIniciado
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AtendimentoFinalizado)
        </dt>
        <dd class="col-sm-10">
            @Model.AtendimentoFinalizado
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Finalizado)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Finalizado)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdAnimal)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdAnimal)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdMedicoVeterinario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdMedicoVeterinario)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { idAtendimento = Model.Id }) |
    <a asp-action="Index">Retornar</a>
</div>
